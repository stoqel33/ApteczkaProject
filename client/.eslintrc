{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "globals": { "document": true },
  "rules": {
    "import/no-named-as-default-member": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 1,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
